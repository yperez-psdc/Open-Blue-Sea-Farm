<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id='subtypes.team_subtypes' model='ir.ui.view'>
			<field name='name'>subtypes_team</field>
			<field name='model'>helpdesk.team</field>
			<field name='inherit_id' ref='helpdesk.helpdesk_team_view_form'/>
			<field name='arch' type='xml'>
				<xpath expr="//form[1]/sheet[1]/div[not(@description)][1]" position="after">
                    <group name="Team claim subtypes" string="Check for claim subtypes">
                        <field name="claim_subtypes" string="Claim subtypes"/>
                    </group>
                </xpath>
			</field>
		</record>
	</data>
</odoo>